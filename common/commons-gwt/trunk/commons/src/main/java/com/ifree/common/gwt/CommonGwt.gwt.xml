<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module>


    <inherits name="com.google.gwt.user.Debug"/>
    <inherits name='com.google.gwt.user.UserAgent'/>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.inject.Inject'/>
    <!--Logging-->
    <inherits name="com.google.gwt.logging.Logging"/>




    <!--gwtp-->
    <inherits name="com.gwtplatform.dispatch.rest.DispatchRest"/>
    <inherits name='com.gwtplatform.mvp.MvpWithEntryPoint'/>
    <inherits name="com.gwtplatform.dispatch.Dispatch"/>


    <inherits name="org.gwtbootstrap3.GwtBootstrap3"/>
    <inherits name="org.gwtbootstrap3.extras.select.Select"/>
    <inherits name="org.gwtbootstrap3.extras.datetimepicker.DateTimePicker"/>

    <entry-point class='com.ifree.common.gwt.client.CommonGwt'/>


    <generate-with class="com.ifree.common.gwt.rebind.PropertyAccessGenerator">
        <when-type-assignable class="com.ifree.common.gwt.shared.PropertyAccess" />
    </generate-with>

    <source path="client" />
    <source path="shared" />
</module>